    "url": "<source\ssrc=\"(.*)\"\stype=\"audio\/\w+\"\s?/>"
  },
  "reddit": {
    "creator": "<title>.*\s:\s(.*)</title>",
    "title": "<title>(.*)\s:\s.*</title>",
    "url": "<link\srel=\"canonical\"\shref=\"(.*)\"?\s/>"
  },
  "twitter": {
