{
  "name": "data",
  "version": "1.0.0",
  "description": "DB backups.",
  "private": true,
  "main": "index.js",
  "bin": {
    "update": "./index.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json",
    "format": "prettier --write index.js",
    "lint": "eslint --fix index.js"
  },
  "lint-staged": {
    "*.+(js|css|ms)": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fourjuaneight/data.git"
  },
  "author": "Juan Villela <juan@villela.co>",
  "bugs": {
    "url": "https://github.com/fourjuaneight/data/issues"
  },
  "homepage": "https://github.com/fourjuaneight/data#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-jcv": "0.1.3",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.1",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0"
  }
}
